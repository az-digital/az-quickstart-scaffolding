{
    "name": "az-digital/az-quickstart-scaffolding",
    "description": "Arizona Quickstart Scaffolding for Composer",
    "type": "project",
    "license": "GPL-2.0-only",
    "authors": [
        {
            "name": "Arizona Digital",
            "homepage": "http://uadigital.arizona.edu/",
            "role": "Team"
        }
    ],
    "repositories": {
        "az_quickstart": {
            "type": "vcs",
            "url": "https://github.com/az-digital/az_quickstart.git",
            "no-api": true
        },
        "0": {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        "1": {
            "type": "composer",
            "url": "https://asset-packagist.org"
        },
        "2": {
            "type": "vcs",
            "url": "https://github.com/az-digital/phpcs-security-audit",
            "no-api": true
        }
    },
    "require": {
        "php": ">=8.3",
        "az-digital/az_quickstart": "dev-issue/3471",
        "composer/installers": "2.3.0",
        "cweagans/composer-patches": "1.7.3",
        "drupal/core-composer-scaffold": "^11.1",
        "drush/drush": "^12.4.3 || ^13.4.0",
        "mglaman/composer-drupal-lenient": "^1.0",
        "oomphinc/composer-installers-extender": "2.0.1",
        "vlucas/phpdotenv": "5.6.0",
        "webflo/drupal-finder": "^1.2.2"
    },
    "require-dev": {
        "az-digital/az-quickstart-dev": "dev-issue/3471",
        "drupal/core-dev": "^11.1"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "composer/installers": true,
            "cweagans/composer-patches": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "drupal/core-composer-scaffold": true,
            "mglaman/composer-drupal-lenient": true,
            "oomphinc/composer-installers-extender": true,
            "php-http/discovery": true,
            "phpstan/extension-installer": true,
            "tbachert/spi": true
        }
    },
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ],
        "files": [
            "load.environment.php"
        ]
    },
    "scripts": {
        "pre-install-cmd": [
            "QuickstartProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "QuickstartProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "QuickstartProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "QuickstartProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "phpcs": [
            "phpcs -p --colors --standard=web/profiles/custom/az_quickstart/phpcs.xml.dist"
        ]
    },
    "extra": {
        "composer-exit-on-patch-failure": true,
        "enable-patching": true,
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/libraries/{$name}": [
                "type:bower-asset",
                "type:drupal-library",
                "type:npm-asset"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "web/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "web/profiles/custom/{$name}": [
                "type:drupal-custom-profile"
            ],
            "web/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ],
            "drush/Commands/contrib/{$name}": [
                "type:drupal-drush"
            ]
        },
        "installer-types": [
            "bower-asset",
            "npm-asset"
        ],
        "drupal-scaffold": {
            "allowed-packages": [
                "az-digital/az_quickstart"
            ],
            "file-mapping": {
                "[project-root]/.editorconfig": {
                    "path": "web/core/assets/scaffold/files/editorconfig",
                    "overwrite": false
                },
                "[project-root]/.gitattributes": {
                    "path": "web/core/assets/scaffold/files/gitattributes",
                    "overwrite": false
                }
            },
            "gitignore": false,
            "locations": {
                "web-root": "web/"
            }
        },
        "drupal-lenient": {
            "allowed-list": [
                "drupal/media_migration",
                "drupal/migrate_queue_importer"
            ]
        }
    }
}
